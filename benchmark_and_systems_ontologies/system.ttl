@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hobbit: <http://w3id.org/hobbit/vocab#> .
@prefix sys: <http://w3id.org/system#> .
@prefix bench: <http://w3id.org/bench#> .
	
# --- System ---
sys:limesV2 a  hobbit:SystemInstance ; 
	rdfs:label	"LIMES"@en;
	rdfs:comment	"LIMES, the Link Discovery Framework for Metric Spaces, is a framework for discovering links between entities contained in Linked Data sources. "@en;
	hobbit:implementsAPI	bench:LinkingAPI;
	hobbit:imageName "linking_system" ;
#	hobbit:imageName "git.project-hobbit.eu:4567/jsaveta1/linkingsystemadapter" ;
	hobbit:instanceOf sys:Limes .
	
sys:Limes a hobbit:System ;
    rdfs:label	"LIMES"@en;
    rdfs:comment "LIMES"@en . 
